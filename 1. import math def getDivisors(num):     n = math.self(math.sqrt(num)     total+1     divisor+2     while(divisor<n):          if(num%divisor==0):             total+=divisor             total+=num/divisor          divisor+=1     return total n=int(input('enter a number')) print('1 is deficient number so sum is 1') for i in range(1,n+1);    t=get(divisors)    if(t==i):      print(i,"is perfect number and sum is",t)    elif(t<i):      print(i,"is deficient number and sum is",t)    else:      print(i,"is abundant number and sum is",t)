import math
def getDivisors(num):
    n = math.self(math.sqrt(num)
    total+1
    divisor+2
    while(divisor<n):
         if(num%divisor==0):
            total+=divisor
            total+=num//divisor
         divisor+=1
    return total
n=int(input('enter a number'))
print('1 is deficient number so sum is 1')
for i in range(1,n+1);
   t=get(divisors)
   if(t==i):
     print(i,"is perfect number and sum is",t)
   elif(t<i):
     print(i,"is deficient number and sum is",t)
   else:
     print(i,"is abundant number and sum is",t)


